<?xml version="1.0" encoding="UTF-8" ?>
<gatein-resources
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.exoplatform.org/xml/ns/gatein_resources_1_4 http://www.exoplatform.org/xml/ns/gatein_resources_1_4"
  xmlns="http://www.exoplatform.org/xml/ns/gatein_resources_1_4">

  <portlet-skin>
    <application-name>documents-portlet</application-name>
    <portlet-name>Documents</portlet-name>
    <skin-name>Enterprise</skin-name>
    <css-path>/skin/css/documents.css</css-path>
  </portlet-skin>

  <portal-skin>
    <skin-name>Enterprise</skin-name>
    <skin-module>Documents</skin-module>
    <css-path>/skin/css/global-documents.css</css-path>
    <css-priority>2</css-priority>
  </portal-skin>

  <portlet>
    <name>Documents</name>
    <module>
      <script>
        <minify>false</minify>
        <path>/js/documents.bundle.js</path>
      </script>
      <depends>
        <module>commonVueComponents</module>
      </depends>
      <depends>
        <module>extensionRegistry</module>
      </depends>
      <depends>
        <module>vue</module>
      </depends>
      <depends>
        <module>vuetify</module>
      </depends>
      <depends>
        <module>eXoVueI18n</module>
      </depends>
      <depends>
        <module>documentPreview</module>
      </depends>
      <depends>
        <module>jquery</module>
        <as>$</as>
      </depends>
      <depends>
        <module>documentsSnackbarComponent</module>
      </depends>
        <depends>
            <module>versionHistoryDrawer</module>
        </depends>
        <depends>
            <module>commons-cometd3</module>
            <as>cCometd</as>
        </depends>
        <depends>
            <module>DocumentsDraggable</module>
        </depends>
        <depends>
            <module>applicationToolbarComponent</module>
        </depends>
    </module>
  </portlet>

  <portlet>
    <name>DocumentsExtensions</name>
    <module>
      <script>
        <path>/js/documentsExtensions.bundle.js</path>
      </script>
      <depends>
        <module>extensionRegistry</module>
      </depends>
      <depends>
        <module>eXoVueI18n</module>
      </depends>
      <depends>
        <module>vue</module>
      </depends>
      <depends>
        <module>commonVueComponents</module>
      </depends>
      <depends>
        <module>documentPreview</module>
      </depends>
    </module>
  </portlet>

  <portlet>
    <name>DownloadDocumentsPublicAccess</name>
    <module>
      <script>
        <minify>false</minify>
        <path>/js/downloadDocumentsPublicAccess.bundle.js</path>
      </script>
      <depends>
        <module>loginCommon</module>
      </depends>
      <depends>
        <module>commonVueComponents</module>
      </depends>
        <depends>
            <module>vue</module>
        </depends>
        <depends>
            <module>vuetify</module>
        </depends>
        <depends>
            <module>eXoVueI18n</module>
        </depends>
        <depends>
            <module>jquery</module>
            <as>$</as>
        </depends>
        <depends>
            <module>extensionRegistry</module>
        </depends>
    </module>
  </portlet>

    <portlet>
        <name>DocumentsSizeGadget</name>
        <module>
            <script>
                <minify>false</minify>
                <path>/js/documentsSizeGadget.bundle.js</path>
            </script>
            <depends>
                <module>commonVueComponents</module>
            </depends>
            <depends>
                <module>vue</module>
            </depends>
            <depends>
                <module>vuetify</module>
            </depends>
            <depends>
                <module>eXoVueI18n</module>
            </depends>
            <depends>
                <module>jquery</module>
                <as>$</as>
            </depends>
        </module>
    </portlet>

    <module>
        <name>documentsSnackbarComponent</name>
        <script>
            <minify>false</minify>
            <path>/js/documentsSnackbarComponent.bundle.js</path>
        </script>
        <depends>
            <module>vue</module>
        </depends>
    </module>

    <module>
        <name>DocumentsDraggable</name>
        <script>
            <minify>false</minify>
            <path>/js/DocumentsDraggable.js</path>
        </script>
        <depends>
            <module>eXoVueI18n</module>
    </depends>
    <depends>
      <module>jquery</module>
      <as>$</as>
    </depends>
  </module>

</gatein-resources>